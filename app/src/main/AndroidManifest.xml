        <activity android:name=".ServiceProviderNewAccountVerify" />
        <activity android:name=".ServiceProviderNewAccount" />
        <activity android:name=".ServiceProviderLogin" />
        <activity android:name=".welcome" />
        <activity android:name=".User_VerifyID_Screen" />
        <activity android:name=".User_Create_Account" />
        <activity android:name=".User_Login" />
        <activity android:name=".UserSignUp" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>